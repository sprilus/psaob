	<queryString>
		<![CDATA[SELECT TO_CHAR (SYSDATE, 'DD/MM/YYYY') DATA_ATTUALE,
       PSSTR.SR_DESC SR_DESC,
       SR_RESPONSABILE DIRETTORE,
       'SAN MICHELE' AZ_DESC,
       'via G. Peretti 09134' SR_IND,
       SR_TEL SR_TEL,
       'CAGLIARI' SR_COMUNE,
       SC_ID SCHEDA,
       SC_SCHEDA NUMEROSCHEDA,
       PZ_CFIS CODICEFISCALE,
       PZ_COGN COGNOME,
       PZ_NOME NOME,
       TRUNC ( (SYSDATE - PZ_DT_NAS) / 365.25) ETA,
       TO_CHAR (PZ_DT_NAS, 'DD/MM/YYYY') DT_NAS,
       comnas.cm_desc COMUNE_NAS,
       comres.cm_desc COMUNE_RES,
       pz_ind_res IND_RES,
       pz_nciv_res NCIV_RES,
       '' TEL1,
       'TODO' SERVIZIOID,
       TO_CHAR (sc_dt_inizio, 'dd/mm/yyyy') DATA_ACCESSO,
       TO_CHAR (sc_dt_inizio, 'hh24:mi') ORA_ACCESSO,
       TO_CHAR (sc_dt_visita, 'dd/mm/yyyy') DATA_VISITA,
       TO_CHAR (sc_dt_visita, 'hh24:mi') ORA_VISITA,
       TO_CHAR (sc_dt_fine, 'dd/mm/yyyy') DATA_DIMISSIONI,
       TO_CHAR (sc_dt_fine, 'hh24:mi') ORA_DIMISSIONI,
       TO_CHAR (
             ''
          || sc_anamnesi_medica1
          || sc_anamnesi_medica2
          || sc_anamnesi_medica3)
          ANAMNESI,
       TO_CHAR (SUBSTR (ps_esame_obiettivo, 0, 4000)) ESAME_OBIETTIVO,
       '---' ESAMI,
       TO_CHAR (PS_TERAPIA_PRATICATA) FARMACO_TESTO,
       'TODO' FARMACO,
       '---' PARAMETRI_CLINICI,
       TO_CHAR (SC_DIAGNOSI_USCITA_TXT) DIAGNOSI,
       TO_CHAR (ps_conclusioni_indiczioni) CONCLUSIONI,
       tipoesito.descrizione MODALITA_DIMISSIONE,
       CASE
          WHEN sc_trg_out = 1 THEN 'ROSSO'
          WHEN sc_trg_out = 2 THEN 'GIALLO'
          WHEN sc_trg_out = 3 THEN 'VERDE'
          WHEN sc_trg_out = 4 THEN 'BIANCO'
          WHEN sc_trg_out = 5 THEN 'NERO'
          ELSE 'ERRORE ' ||  sc_trg_out
       END
          COLORE_ESITO,
       ' ' REP_RICOVERO_ID,
       ' ' REP_RICOVERO,
       ' ' STRTRASF,
       medicoacc.first_name MEDICO_ACC_NOME,
       medicoacc.last_name MEDICO_ACC_COGN,
       'TODO' DIVDESC,
       ' ' NOSOLOGICO,
       PS_PROGNOSI_GG_LAV PROGNOSI_LAV,
       PS_PROGNOSI_GG_CLINICI PROGNOSI_CLI,
       '' SC_F_RISER,
       'N' SC_F_CHIUSODATRG,
       medicodim.first_name MEDICO_DIM_NOME,
       medicodim.last_name MEDICO_DIM_COGN,
       TO_CHAR (sc_note_prescrizioni) NT_DESC,
       sc_tipo_esito_codice SC_ESITO_ID,
       PROBL_PRINC.DESCRIZIONE AS PROBLEMA_PRINC
  FROM PSAOB_PS_SCHEDA
       JOIN psaob_paziente paziente
          ON sc_paziente = paziente.id
       LEFT JOIN PSAOB_PS_PROBLEMA_PRINCIPALE PROBL_PRINC
          ON SC_PROBLEMA_PRINC_ID = PROBL_PRINC.ID
       LEFT JOIN psaob_sa_comune comnas
          ON pz_com_nas_cmid = comnas.cmid
       LEFT JOIN psaob_sa_comune comres
          ON pz_com_res_cmid = comres.cmid
       LEFT JOIN psaob_sa_struttura psstr
          ON sc_struttura_srid = psstr.srid
       LEFT JOIN sec_user medicoacc
          ON PS_MEDICO_ACC_ID = medicoacc.id
       LEFT JOIN sec_user medicodim
          ON PS_MEDICO_dim_ID = medicodim.id
       LEFT JOIN PSAOB_PS_TIPO_ESITO_TAB tipoesito
          ON sc_tipo_esito_codice = tipoesito.codice
 WHERE SC_ID =  8]]>
	</queryString>
